extends layout

block content
	div(class='table-responsive', id='div--cardsPlayer')
		table(class='table table-striped bg-white')
				thead
					tr
						th Carta
						th Nombre
						th Level
						th Accion
				tbody(id='tbody--query')
		
		div(class='clearfix d-none' id='div--spinner')
			div(class='spinner-border float-right text-primary' role='status')
				span(class='sr-only') Loading...
